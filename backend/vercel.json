{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.py"
    },
    {
      "src": "/api/health",
      "dest": "api/index.py"
    },
    {
      "src": "/api/token",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "FRONTEND_ORIGIN": "https://your-frontend-domain.vercel.app"
  }
}