{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.py"
    },
    {
      "src": "/api/health",
      "dest": "api/index.py"
    },
    {
      "src": "/api/token",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "FRONTEND_ORIGIN": "https://your-frontend.vercel.app",
    "LIVEKIT_URL": "wss://voyageai-3t6pxcz2.livekit.cloud",
    "LIVEKIT_API_KEY": "@livekit_api_key",
    "LIVEKIT_API_SECRET": "@livekit_api_secret",
    "DEEPGRAM_API_KEY": "@deepgram_api_key",
    "CARTESIA_API_KEY": "@cartesia_api_key",
    "GOOGLE_API_KEY": "@google_api_key"
  }
}